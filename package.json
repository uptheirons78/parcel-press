{
  "name": "parcel-press",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "parcel dev parcel-imports.js -o main",
    "build": "parcel build parcel-imports.js -o main --public-url ./",
    "watch": "parcel watch parcel-imports.js -o main --public-url ./",
    "serve": "parcel serve parcel-imports.js -o main --public-url ./",
    "production": "parcel build parcel-imports.js -o main --no-cache --no-source-maps --public-url ./"
  },
  "watchreload": {
    "files": [
      "**/*.php",
      "**/*.js",
      "**/*.css"
    ]
  },
  "keywords": [
    "wordpress",
    "parcel"
  ],
  "author": "Mauro Bono",
  "license": "MIT",
  "devDependencies": {
    "normalize.css": "^8.0.1",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-watch-reload": "^1.0.3",
    "sass": "^1.41.0"
  }
}
